    "vue": "^3.2.45",
    "vue-tsc": "^1.0.16"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^9.8.2",
    "@vueuse/electron": "^9.9.0",
    "element-plus": "^2.2.27",
    "iconv-lite": "^0.6.3",
    "js-yaml": "^4.1.0",
    "vue-router": "^4.1.6",
